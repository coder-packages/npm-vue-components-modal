{
  "name": "coder-vue-components-modal",
  "version": "1.0.3",
  "description": "Vue modal",
  "main": "dist/modal.umd.js",
  "module": "dist/modal.esm.js",
  "unpkg": "dist/modal.min.js",
  "browser": {
    "./sfc": "src/modal.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/modal.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/modal.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/modal.min.js"
  },
  "keywords": [
    "modal",
    "vue"
  ],
  "author": "Alexander Manukian <alikmanukian@gmail.com>",
  "dependencies": {
    "css-loader": "^0.28.0",
    "node-sass": "^4.9.2",
    "portal-vue": "^1.5.1",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.0.3",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2"
  }
}
